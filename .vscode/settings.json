{
  "workbench.colorTheme": "GitHub Light",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorStyle": "line",
  "editor.detectIndentation": true,
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.guides.indentation": true,
  "editor.insertSpaces": true,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "off",
  "editor.tabSize": 2,
  "files.autoGuessEncoding": true,
  "files.autoSave": "onWindowChange",
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.exclude": {
    "**/__pycache__": true,
    "**/_minted-*/": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/*.acn": true,
    "**/*.acr": true,
    "**/*.alg": true,
    "**/*.aux": true,
    "**/*.bbl": true,
    "**/*.bcf": true,
    "**/*.blg": true,
    "**/*.fdb_latexmk": true,
    "**/*.fls": true,
    "**/*.glg": true,
    "**/*.glo": true,
    "**/*.gls": true,
    "**/*.glx": true,
    "**/*.gxg": true,
    "**/*.gxs": true,
    "**/*.hd": true,
    "**/*.idx": true,
    "**/*.ilg": true,
    "**/*.ind": true,
    "**/*.ist": true,
    "**/*.lof": true,
    "**/*.log": true,
    "**/*.lot": true,
    "**/*.nav": true,
    "**/*.o": true,
    "**/*.obj": true,
    "**/*.out": true,
    "**/*.pygtex": true,
    "**/*.run.xml": true,
    "**/*.snm": true,
    "**/*.synctex.gz": true,
    "**/*.toc": true,
    "**/*.url": true,
    "**/*.vrb": true,
    "**/*.xdv": true,
    "**/CVS": true,
    "**/Thumbs.db": true,
  },
  "search.exclude": {
    "**/.env": true,
    "**/.git": true,
    "**/.github": false,
    "**/.nuxt": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.venv": true,
    "**/.vitepress/.cache": true,
    "**/.vitepress/.temp": true,
    "**/.vitepress/dist": true,
    "**/.vscode": true,
    "**/.vuepress/.cache": true,
    "**/.vuepress/.temp": true,
    "**/.vuepress/dist": true,
    "**/.yarn": true,
    "**/*.code-search": true,
    "**/*.snap": true,
    "**/*.svg": true,
    "**/assets": true,
    "**/bower_components": true,
    "**/cache": true,
    "**/CHANGELOG*": true,
    "**/dist/**": true,
    "**/LICENSE*": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/pnpm-lock.yaml": true,
    "**/public": true,
    "**/temp": true,
    "**/yarn.lock": true,
  },
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.tabSize": 4,
    "editor.quickSuggestions": {
      "other": "off",
      "comments": "off",
      "strings": "off"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[plaintext]": {
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "editor.wordWrap": "on"
  },
  "MarkdownPaste.path": "${fileDirname}/img"
}